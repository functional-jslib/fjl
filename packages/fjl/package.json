{
  "name": "fjl",
  "version": "1.13.0",
  "description": "Functional Javascript Library.",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "typings": "dist/es/index.d.ts",
  "scripts": {
    "build": "npm run build:all",
    "build:all": "npm run build:es && npm run build:cjs && npm run build:umd && npm run build:iife",
    "test": "jest --config ./jest.config.js",
    "docs": "echo \"Not yet implemented.\"",
    "build:es": "tsc --project tsconfig.prod.es.json",
    "build:cjs": "tsc --project tsconfig.prod.cjs.json",
    "build:umd": "tsc --project tsconfig.prod.umd.json",
    "build:iife": "rollup -c ./rollup.config.js",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@types/jest": "^26.0.16",
    "@types/node": "^13.13.32",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "del": "^5.1.0",
    "eslint": "^6.8.0",
    "jest": "^26.6.3",
    "rollup": "^2.33.3",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/functional-jslib/fjl.git"
  },
  "keywords": [
    "functional",
    "fp",
    "programming",
    "haskell",
    "prelude",
    "combinators"
