{
  "name": "fjl-monorepo",
  "version": "0.0.0",
  "description": "",
  "private": true,
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-typescript": "^8.2.0",
    "@types/jest": "^26.0.21",
    "@types/node": "^13.13.47",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "del": "^5.1.0",
    "eslint": "^6.8.0",
    "jest": "^27.0.4",
    "jest-cli": "^27.0.4",
    "rollup": "^2.50.6",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^27.0.2",
    "ts-node": "^9.1.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "scripts": {
    "test": "npm run test:fjl",
    "test:fjl": "jest -c packages/fjl/jest.config.js",
    "test:fjl-validator": "jest --c packages/fjl-validator/jest.config.js",
    "test:fjl-inputfilter": "jest --c packages/fjl-inputfilter/jest.config.js",
    "test:fjl-filter": "jest --c packages/fjl-filter/jest.config.js",
    "build": "npm run build:fjl",
    "build:fjl": "npm run test:fjl && npm run build:fjl:cjs && npm run build:fjl:esm && npm run build:fjl:iife && npm run build:fjl:umd && npm run build:fjl:min",
    "build:fjl:cjs": "tsc --project packages/fjl/tsconfig.prod.cjs.json",
    "build:fjl:esm": "tsc --project packages/fjl/tsconfig.prod.es.json",
    "build:fjl:iife": "tsc --project packages/fjl/tsconfig.json",
    "build:fjl:umd": "tsc --project packages/fjl/tsconfig.prod.umd.json",
    "build:fjl:min": "rollup --config packages/fjl/rollup.config.js",
    "build:fjl-validator": "npm run test:fjl-validator && npm run build:fjl-validator:cjs && npm run build:fjl-validator:esm && npm run build:fjl-validator:iife && npm run build:fjl-validator:umd && npm run build:fjl-validator:min",
    "build:fjl-validator:cjs": "tsc --project packages/fjl-validator/tsconfig.prod.cjs.json",
    "build:fjl-validator:esm": "tsc --project packages/fjl-validator/tsconfig.prod.es.json",
    "build:fjl-validator:iife": "tsc --project packages/fjl-validator/tsconfig.json",
    "build:fjl-validator:umd": "tsc --project packages/fjl-validator/tsconfig.prod.umd.json",
    "build:fjl-validator:min": "rollup --config packages/fjl-validator/rollup.config.js",
    "link:packages": "node node-scripts/tasks/link-packages.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/functional-jslib/fjl.git"
  },
  "keywords": [],
  "author": "Ely De La Cruz <elycruz@elycruz.com>",
  "license": "BSD",
  "bugs": {
    "url": "https://github.com/functional-jslib/fjl/issues"
  },
  "homepage": "https://github.com/functional-jslib/fjl#readme",
  "workspaces": [
    "packages/fjl",
    "packages/fjl-validator"
  ]
}
