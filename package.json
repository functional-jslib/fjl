{
  "name": "fjl",
  "version": "1.12.12",
  "description": "Functional Javascript Library",
  "main": "dist/cjs/fjl",
  "module": "dist/es6-module/fjl.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "build": "npm run rollup && npm run docs",
    "docs": "npm run jsdoc",
    "jsdoc": "jsdoc -c ./jsdoc.config.json ./src/",
    "rollup": "del-cli dist/ && rollup --config rollup.config.js",
    "eslint": "eslint ./src/",
    "test": "npm run eslint && jest --testMatch **/tests/test-*.js",
    "prepublishOnly": "npm run test && npm run build"
  },
  "directories": {
    "dist": "dist",
    "doc": "README.md",
    "test": "tests",
    "lib": "src"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/plugin-external-helpers": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/plugin-syntax-bigint": "^7.8.3",
    "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.7",
    "@types/jest": "^25.1.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^25.1.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-preset-es2015-rollup": "^3.0.0",
    "del": "^5.1.0",
    "del-cli": "^4.0.1",
    "eslint": "^8.14.0",
    "jest": "^27.5.1",
    "jsdoc": "^3.6.10",
    "rollup": "^2.70.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "jest": {
    "testMatch": [
      "**/tests/test-*.js"
    ],
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/functional-jslib/fjl.git"
  },
  "keywords": [
    "functional programming",
    "functional",
    "fp",
    "programming",
    "haskell",
    "prelude",
    "combinators"
  ],
  "author": "Ely De La Cruz <elycruz@elycruz.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/functional-jslib/fjl/issues"
  },
  "homepage": "https://github.com/functional-jslib/fjl#readme",
  "buildConfig": {
    "inputModuleName": "fjl",
    "inputFilePath": "./src/fjl.js",
    "outputFileName": "fjl.js",
    "outputFileNameMjs": "fjl.mjs",
    "outputFileNameMin": "fjl.min.js",
    "srcsGlob": "./src/**/*.js",
    "paths": {
      "dist": "./dist",
      "docs": "./docs"
    },
    "folderNames": {
      "amd": "amd",
      "cjs": "cjs",
      "iife": "iife",
      "umd": "umd",
      "es6Module": "es6-module",
      "package": "package"
    },
    "es6ModuleRollup": {
      "config": {
        "input": "./src/fjl.js",
        "external": []
      },
      "write": {
        "file": "./dist/es6-module/fjl.js",
        "format": "es",
        "sourcemap": true
      }
    },
    "buildAmdOptions": {
      "babel": {
        "babelrc": false,
        "presets": [
          "@babel/preset-env"
        ],
        "plugins": [
          "@babel/plugin-transform-modules-amd"
        ],
        "exclude": [
          "node_modules/**/*"
        ]
      }
    },
    "buildCjsOptions": {
      "babel": {
        "babelrc": false,
        "presets": [
          "@babel/preset-env"
        ],
        "plugins": [
          "@babel/plugin-transform-modules-commonjs"
        ],
        "exclude": [
          "node_modules/**/*"
        ]
      }
    },
    "buildUmdOptions": {
      "babel": {
        "babelrc": false,
        "presets": [
          "@babel/preset-env"
        ],
        "plugins": [
          "@babel/plugin-transform-modules-umd"
        ],
        "exclude": [
          "node_modules/**/*"
        ]
      }
    },
    "buildIifeOptions": {
      "babel": {
        "babelrc": false,
        "presets": [
          [
            "@babel/preset-env",
            {
              "modules": false
            }
          ]
        ],
        "exclude": [
          "node_modules/**/*"
        ]
      }
    },
    "uglifyOptions": {},
    "readme": {
      "files": [
        "./markdown-fragments/readme-header.md",
        "./markdown-fragments/sections/sections.md",
        "./markdown-fragments/sections/requirements.md",
        "./markdown-fragments/sections/getting-started.md",
        "./markdown-fragments/sections/docs.md",
        "./markdown-fragments-generated/module-and-member-list.md",
        "./markdown-fragments/sections/docs/docs-footer.md",
        "./markdown-fragments/sections/development.md",
        "./markdown-fragments/sections/license.md",
        "./markdown-fragments/sections/resources.md",
        "./markdown-fragments/sections/changelog.md"
      ]
    }
  }
}
